commands:
    01_fetch_sh:
        test: '[ ! -f /usr/bin/node ] && echo "node not installed"'
        command: "curl -sL https://rpm.nodesource.com/setup_16.x | sudo bash -"
    02_install_latest_node:
        test: '[ ! -f /usr/bin/node ] && echo "node not installed"'
        command: "sudo yum install -y nodejs"